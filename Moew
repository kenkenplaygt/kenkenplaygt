    wait(0.1)


    local bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.MaxForce = Vector3.new(100000, 100000, 100000)  -- Max force to apply the movement
    bodyVelocity.Parent = humanoidRootPart

